// First interface
interface Writer {
    void write();
}
// Second interface
interface Reader {
    void read();
}
// Class implementing both interfaces
class Harsh implements Writer, Reader {
    public void write() {
        System.out.println("Harsh is writing Java code.");
    }

    public void read() {
        System.out.println("Harsh is reading Java notes.");
    }
}
// Main class
public class MultipleInheritance {
    public static void main(String[] args) {
        Harsh s = new Harsh();
        s.write();
        s.read();
    }
}
