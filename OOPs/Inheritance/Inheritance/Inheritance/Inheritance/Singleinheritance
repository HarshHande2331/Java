// Parent class
class Person {
    void speak() {
        System.out.println("Person can speak.");
    }
}

// Child class
class Harsh extends Person {
    void study() {
        System.out.println("Harsh is studying Java.");
    }
}

// Main class
public class Singleinheritance {
    public static void main(String[] args) {
        Harsh s = new Harsh();
        s.speak();   // Inherited from Person
        s.study();   // Own method of Harsh
    }
}
